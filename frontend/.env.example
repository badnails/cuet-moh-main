# Sentry DSN for error tracking
VITE_SENTRY_DSN=your-sentry-dsn-here

# Sentry project URL (for dashboard links)
VITE_SENTRY_URL=https://sentry.io

# API base URL (optional, defaults to /api which uses Vite proxy)
# VITE_API_URL=http://localhost:3000

# Jaeger UI URL (for trace viewer links)
VITE_JAEGER_URL=http://localhost:16686

# OpenTelemetry collector endpoint (optional)
VITE_OTEL_COLLECTOR_URL=http://localhost:4318

# Environment
VITE_APP_ENV=development
